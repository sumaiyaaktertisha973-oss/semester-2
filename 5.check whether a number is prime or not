#include <iostream>
using namespace std;
int main(){
    int n, i, flag = 0;
    cout << "enter a positive number: ";
    cin >> n;
    if (n <= 1){
        cout << "not prime";
        return 0;}
        for (i = 2; i <= n / 2; i++){
            if (n % i == 0){
                flag = 1;
                break;
            }
        }
        if (flag == 0)
        cout << "primre number";
        else
        cout << "not prime";
        return 0;
}
